{
  "name": "viewDeploymentSearchView",
  "title": "视图查询",
  "type": "search",
  "data": "tbl_view_deployment",
  "fields": [
    {
      "name": "name",
      "label": "视图名称",
      "type": "text",
      "operator": "$like"
    },
    {
      "name": "type",
      "label": "视图类型",
      "type": "select",
      "option": {
        "values": {
          "create": "create",
          "search": "search",
          "menu": "menu",
          "hidden": "hidden",
          "chart": "chart",
          "upload": "upload"
        }
      }
    }
  ],
  "result": [
    {
      "name": "name",
      "label": "option配置名称",
      "attributes": {
        "data-sortable": true
      }
    },
    {
      "name": "type",
      "label": "视图类型",
      "attributes": {
        "data-sortable": true
      }
    }
  ],
  "actions": [
    {
      "search": true,
      "style":"btn-primary"
    }
  ],
  "operate": [
