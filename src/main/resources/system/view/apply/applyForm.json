{
  "title": "项目申请",
  "type": "create",
  "data": "tbl_project",
  "name": "applyForm",
  "process": {
    "name": "My process",
    "output": {
      "applyGroup": "Administrator"
    }
  },
  "section": [
    {
      "name": "main",
      "title": "基本信息",
      "span": 12
    }
  ],
  "fields": [
    {
      "name": "project",
      "type": "text",
      "label": "项目名称",
      "attributes": {
        "class": "form-control"
      },
      "container": {
        "span": 6,
        "label": 4,
        "widget": 8
      },
      "section": "main",
      "tip": "项目名称是项目的唯一标识，一旦确定不能休息"
    },
    {
      "name": "projectID",
      "type": "text",
      "label": "项目编码",
      "attributes": {
        "class": "form-control",
        "placeholder": "请输入项目编码"
      },
      "section": "main",
      "tip": "项目编码格式: YYYYMMDD-CODE"
    },
    {
      "name": "coordinationType",
      "type": "radio",
      "label": "协作类型",
      "attributes": {
        "class": "radio",
        "value": "YWS"
      },
      "container": {
        "class": "col-md-4"
      },
      "option": {
        "values": {
          "YWS": "专业协作",
          "YWE": "人员借用",
          "YWP": "项目承接",
          "YWT": "客户经理协作"
        }
      }
    },
    {
      "name": "tradeMode",
      "type": "radio",
      "label": "交易方式",
      "attributes": {
        "class": "radio",
        "value": ""
      },
      "option": {
        "values": {
          "direct": "定向交易",
          "consult": "交易征询",
          "appoint": "公司指派"
        }
      }
    },
    {
      "name": "initiateMode",
      "type": "select",
      "label": "发起方式",
      "attributes": {
        "class": "form-control",
        "value": "YWS"
      },
      "option": {
        "values": {
          "YWS": "专业协作",
          "YWE": "人员借用",
          "YWP": "项目承接",
          "YWT": "客户经理协作"
        }
      }
    },
    {
      "name": "initiator",
      "type": "text",
      "label": "发起人",
      "attributes": {
        "class": "form-control"
      },
      "container": {
        "class": "col-md-6"
      },
      "section": "发起人"
    },
    {
      "name": "initiateDeptment",
      "type": "text",
      "label": "发起部门",
      "attributes": {
        "class": "form-control"
      }
    },
    {
      "name": "initiateDeptmentLeader",
      "type": "text",
      "label": "发起部门领导",
      "attributes": {
        "class": "form-control"
      }
    },
    {
      "name": "projectDesc",
      "type": "textarea",
      "label": "项目内容",
      "attributes": {
        "class": "form-control"
      },
      "container": {
        "span": 12,
        "label": 2,
        "widget": 10
      }
    },
    {
      "name": "coordinateContent",
      "type": "richtext",
      "label": "协作内容",
      "attributes": {
        "class": "form-control"
      }
    }
  ],
  "validators": [
    {
      "field": "project",
      "provider": "required",
      "msg": "required"
    }
  ],
  "permission": [],
  "redirect": "/spring/data/search/processSearchView",
  "actions": [
    {
      "submit": true,
      "style":"btn-primary"
    }
  ]
}